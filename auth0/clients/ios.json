{
  "is_token_endpoint_ip_header_trusted": false,
  "name": "Courier iOS",
  "is_first_party": true,
  "oidc_conformant": true,
  "sso_disabled": false,
  "cross_origin_auth": false,
  "allowed_clients": [],
  "allowed_logout_urls": [
    "com.mattmoriarity.Courier://##AUTH_DOMAIN/ios/com.mattmoriarity.Courier/callback"
  ],
  "callbacks": [
    "com.mattmoriarity.Courier://##AUTH_DOMAIN##/ios/com.mattmoriarity.Courier/callback"
  ],
  "native_social_login": {
    "apple": {
      "enabled": false
    }
  },
  "jwt_configuration": {
    "alg": "RS256",
    "lifetime_in_seconds": 36000,
    "secret_encoded": false
  },
  "client_aliases": [],
  "token_endpoint_auth_method": "none",
  "app_type": "native",
  "grant_types": ["authorization_code", "implicit", "refresh_token"],
  "custom_login_page_on": true
}
